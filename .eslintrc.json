{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "react/no-unescaped-entities": [
      "error",
      {
        "forbid": [">", "}"]
      }
    ],
    "spaced-comment": [
      "error",
      "always",
      { "exceptions": ["-", "*"], "markers": ["/"] }
    ],
    "react/display-name": "off"
  }
}
